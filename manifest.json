{
	"manifest_version": 2,
	"name": "Vertauschte Wörter!",
	"version": "1.0.0",

	"description": "Vertauscht Wörter auf Webseiten.",

	"icons": {
		"16": "img/icon_16.png",
		"48": "img/icon_48.png",
		"96": "img/icon_96.png",
		"128": "img/icon_128.png"
	},

	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": [
			"js/plugin.js"
		]
	}],

	"permissions": [
	     "tabs"
	]
}
